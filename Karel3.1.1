void partyAgain()
{
    turnLeft();
    pickBeeper();
    goForward();
    turnLeft(); //nach dropStars
    if(frontIsClear())
    {
        moveForward();
        partyAgain();
    }
}

void goForward()
{
    if(frontIsClear())
    {
        moveForward();
        goForward();
        dropStars();
    }
    else
    {
        dropBeeper();
    }
}

void dropStars()
{
    if(onBeeper())
    {
        turnAround();
    }
    moveForward();
}
